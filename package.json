{
  "name": "jimmer-code-gen-vue3",
  "private": true,
  "version": "0.5.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",

    "api": "tsx scripts/api/generateApi.ts",

    "type-gen": "tsx scripts/precompile/generateTypes.ts && vue-tsc --build --force",
    "default-script-gen": "tsx scripts/backend/generateDefaultScript.ts",
    "default-type-mapping-gen": "tsx scripts/backend/generateDefaultTypeMapping.ts"
  },
  "dependencies": {
    "@progress/jszip-esm": "^1.0.4",
    "@typescript/vfs": "^1.6.1",
    "@vue-flow/core": "^1.46.5",
    "@vue-flow/node-toolbar": "^1.1.1",
    "ajv": "^8.17.1",
    "clipboard-polyfill": "^4.1.1",
    "file-saver": "^2.0.5",
    "html-to-image": "1.11.11",
    "json5": "^2.2.3",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.45.0",
    "prismjs": "^1.30.0",
    "typescript": "~5.9.0",
    "uuid": "^11.1.0",
    "vite-plugin-prismjs": "^0.0.8",
    "vue": "^3.5.22",
    "vue-color": "^3.3.1",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/adm-zip": "^0.5.7",
    "@types/file-saver": "^2.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.18.6",
    "@types/prismjs": "^1.26.5",
    "@types/tinycolor2": "^1.4.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "adm-zip": "^0.5.16",
    "fs-extra": "^11.3.0",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "tsx": "^4.21.0",
    "typescript-json-schema": "^0.65.1",
    "vite": "^7.2.6",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
